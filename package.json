{
  "name": "ens-orb",
  "version": "1.0.1",
  "homepage": "https://ens.rest/",
  "private": false,
  "description": "A bulk query application for the Ethereum Name Service network.",
  "main": "index.js",
  "scripts": {
    "web": "node index.js",
    "start": "webpack serve",
    "watch": "webpack --watch",
    "build": "NODE_ENV=production webpack",
    "build-dev": "webpack"
  },
  "engines": {
    "node": "17.1.0"
  },
  "author": "alrubam",
  "license": "ISC",
  "bugs": "https://github.com/alruban/ens-orb/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/alruban/ens-orb"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "assert": "^2.0.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^5.0.1",
    "html-webpack-plugin": "^5.5.0",
    "https-browserify": "^1.0.0",
    "mini-css-extract-plugin": "^1.3.3",
    "os-browserify": "^0.3.0",
    "postcss": "^8.2.1",
    "postcss-custom-media": "^8.0.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-inline-svg": "^5.0.0",
    "postcss-loader": "^4.1.0",
    "postcss-mixins": "^9.0.1",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.4.2",
    "postcss-pxtorem": "^6.0.0",
    "postcss-sassy-mixins": "^2.1.0",
    "postcss-simple-vars": "^6.0.3",
    "process": "^0.11.10",
    "react-app-rewired": "^2.2.1",
    "react-hot-loader": "^4.13.0",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.0",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "postcss": {
    "plugins": [
      {
        "postcss-easy-import": {
          "extensions": ".pcss",
          "prefix": "_"
        }
      },
      {
        "postcss-inline-svg": {
          "paths": [
            "src/svgs"
          ]
        }
      },
      {
        "postcss-pxtorem": {
          "propList": [
            "font",
            "font-size",
            "line-height",
            "letter-spacing"
          ]
        }
      },
      "postcss-custom-media",
      "postcss-sassy-mixins",
      "postcss-nested",
      "postcss-simple-vars",
      "autoprefixer"
    ]
  },
  "dependencies": {
    "@analytics/google-analytics": "^1.0.7",
    "@babel/runtime": "^7.17.2",
    "analytics": "^0.8.9",
    "graphql-request": "^4.0.0",
    "json2csv": "^4.5.4",
    "minimist": "^1.2.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-csv": "^2.2.2",
    "react-dom": "^17.0.2",
    "react-head": "^3.4.0",
    "react-helmet-async": "^1.2.3",
    "react-router-dom": "^6.2.2",
    "react-svg": "^14.1.13",
    "react-table": "^7.7.0",
    "universal-cookie": "^4.0.4",
    "web3": "^1.2.4"
  }
}
